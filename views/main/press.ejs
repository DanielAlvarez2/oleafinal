    <style>
        #info-page-flexbox{
            display:flex;
            gap:75px;
        }
        #left{
            max-width:53ch;
        }
        #right{
            display:flex;
            flex-direction:column;
            align-items:center;
            row-gap:40px;
        }
        #right img{
            max-width:175px;
        }
        h2{
            margin: 70px 0 20px;
        }
        .coe{
            position:relative;
            color:black;
            font-size:10px;
            width:152px;
            height:125px;
        }
        .coe a,
        .coe a:link,
        .coe a:visited,
        .coe a:hover,
        .coe a:active{
            color:black;
        }
        .coe span{
            width:152px;
            font-family:sans-serif;
            font-weight:700;
            letter-spacing:0px;
            line-height:13px;
            text-align:center;
            position:absolute;
            z-index:100;
        }
        .coe img{
            margin:0 auto;
            position:absolute;
            inset:0;
        }
        .uline{
            text-decoration:underline;
            font-weight:100;
            text-align:center;
            position:absolute;
        }
        @media(max-width:1000px){
            #info-page-flexbox{
                padding-left:5vw;
                padding-right:5vw;
            }
        }
        @media(max-width:700px){
            #info-page-flexbox{
                flex-direction:column;
            }
        }
    </style>
</head>
