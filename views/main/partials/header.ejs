        <script>
            const plusInfo = document.querySelector('#plus-info')
            const minusInfo = document.querySelector('#minus-info')
            const mobileInfoMenu = document.querySelector('#mobile-info-menu')
            plusInfo.addEventListener('click',()=>{
                mobileInfoMenu.style.display = 'block'
                plusInfo.style.display = 'none'
            })
            minusInfo.addEventListener('click',()=>{
                mobileInfoMenu.style.display = 'none'
                plusInfo.style.display = 'block'
            })
            const plusMenus = document.querySelector('#plus-menus')
            const minusMenus = document.querySelector('#minus-menus')
            const mobileMenusMenu = document.querySelector('#mobile-menus-menu')
            plusMenus.addEventListener('click',()=>{
                mobileMenusMenu.style.display = 'block'
                plusMenus.style.display = 'none'
            })
            minusMenus.addEventListener('click',()=>{
                mobileMenusMenu.style.display = 'none'
                plusMenus.style.display = 'block'
            })
            const navMobile = document.querySelector('#nav-mobile')
            const hamburgerMenu = document.querySelector('.fa-bars')
            const closeMenu = document.querySelector('.fa-rectangle-xmark')
            hamburgerMenu.addEventListener('click',()=>{
                navMobile.classList.toggle('hide-mobile-nav')
                closeMenu.classList.toggle('hide-mobile-nav')
                hamburgerMenu.classList.toggle('hide-mobile-nav')
                })
            closeMenu.addEventListener('click',()=>{
                navMobile.classList.toggle('hide-mobile-nav')
                closeMenu.classList.toggle('hide-mobile-nav')
                hamburgerMenu.classList.toggle('hide-mobile-nav')
            })
